class Student:
    name = input('enter the name: ') # class attribute
    def __init__(self):
        self.age = 20  # instance attribute

    @classmethod
    def tostring(cls):
        print('Student Class Attributes: name=',cls.name)

std = Student()
std.tostring()



class Student:
    name = input('enter the name:') # class attribute
    
    def __init__(self):
        self.age = 20  # instance attribute

    @staticmethod
    def tostring():
        print('Student Class')

stud=Student()
stud.tostring()
